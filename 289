#include <iostream>

using namespace std;

void NhapMang(int *a, int n)
{
    for(int i=0; i<n; i++)
    {
        cin >> a[i];
    }
}



void XuatMang(int *a, int n)
{
    for(int i=0; i<n; i++)
    {
        if(a[i] % 2 == 0)
        {
            cout << a[i] << " ";
        }
    }
    cout << endl;
    for(int i=0; i<n; i++)
    {
        if(a[i] % 2 != 0)
        {
            cout << a[i] << " ";

        }
    }
}
/*

*/
int main()
{
    int n;
    cin >> n ;
    int a[n];
    NhapMang(a, n);

    XuatMang(a,n);
}
