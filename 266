#include <iostream>
#include <cmath>
using namespace std;

void Nhapmang(int *a, int n)
{
    for(int i=0; i<n; i++)
    {
        cin >> a[i];
    }
}
void themgiatrix(int *a, int n, int k, int x)
{
    int m=n+1;
    int a_1[m];
    for(int i=0; i<n; i++)
    {
        a_1[i]=a[i];
    }
    for(int j=m-2; j>=k; j--)
    {
        a_1[j+1]=a_1[j];
    }
    a_1[k]=x;
    for(int i=0; i<m; i++)
    {
        cout << a_1[i] << " ";
    }
}

int main()
{
    int n, x, k;
    cin >> n;
    int a[n];
    Nhapmang(a, n);
    cin >> x >> k;
    themgiatrix(a,n,k,x);
}
