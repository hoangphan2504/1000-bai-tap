
#include <iostream>
#include <math.h>

using namespace std;

void NhapMang(float *a, int n)
{
    for(int i=0; i<n; i++)
    {
        cin >> a[i];
    }
}

void LamTron(float *a, int n)
{
    for(int i=0; i<n; i++)
    {
         a[i]=round(a[i]);
    }

}
void XuatMang(float *a, int n)
{
    for(int i=0; i<n; i++)
    {
        cout << a[i] << " ";
    }
    cout << endl;
}


int main()
{
    int n, Max, Min;
    cin >> n;
    float a[n];
    NhapMang(a, n);
    XuatMang(a,n);
    LamTron(a, n);
    XuatMang(a,n);
}
