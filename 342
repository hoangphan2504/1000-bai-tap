#include <iostream>

using namespace std;

void NhapMang(int arr[][100], int &row, int &col)
{
    for(int i=0; i<row; i++)
    {
        for(int j=0; j<col; j++)
        {
            cin >> arr[i][j];
        }
    }
}

int KiemTra(int arr[][100], int row, int col)
{
  int dem=0;
  for(int i=0; i<row; i++)
  {
    for(int j=0; j<col; j++)
    {
      if(arr[i][j] > arr[i][j+1] && arr[i][j] > arr[i][j-1])
      {
        dem++;
      }
    }
  } 
  return dem;
}

void XuatMang(int arr[][100], int row, int col)
{
    for(int i=0; i<row; i++)
    {
        for(int j=0; j<col; j++)
        {
            cout << arr[i][j] << " ";
        }
        cout << endl;
    }
}

int main()
{
    int row, col, n;
    cin >> row >> col;
    int arr[100][100];
    NhapMang(arr, row, col);
    XuatMang(arr, row, col);
    cout <<"So Luong: ";
    cout << KiemTra(arr, row, col);
}
